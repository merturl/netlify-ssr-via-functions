{
  "name": "netlify-sssr-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.2.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "isomorphic-fetch": "^2.2.1",
    "md5": "^2.2.1",
    "morgan": "^1.9.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-hot-loader": "^4.3.12",
    "react-static": "^6.0.18",
    "serverless-http": "^1.6.0",
    "styled-components": "^4.1.3"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=dev npm-run-all --parallel start:app start:server",
    "start:app": "react-static start",
    "start:server": "netlify-lambda serve functions",
    "build": "npm-run-all --parallel build:**",
    "build:app": "react-static build",
    "build:functions": "netlify-lambda build functions",
    "build:ssr": "parcel build functions/react-app/bundle.js --out-dir dist/dev",
    "test": "react-scripts test --env=jsdom"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@types/node": "^11.9.4",
    "@types/reach__router": "^1.2.2",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-hot-loader": "^4.1.0",
    "@types/webpack-env": "^1.13.6",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "netlify-lambda": "^1.4.2",
    "npm-run-all": "^4.1.3",
    "react-static-plugin-typescript": "^3.1.0",
    "serve": "10.1.2"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
}
